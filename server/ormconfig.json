{
    "type": "mongodb",
    "host": "mongo",
    "port": "27017",
    "database": "transaction",
    "useUnifiedTopology": true,
    "synchronize": true,
    "logging": true,
    "entities": [
      "src/entity/*{.js,.ts}"
    ],
    "subscribers": [
      "src/subscriber/*{.js,.ts}"
    ],
    "migrations": [
      "src/migration/*{.js,.ts}"
    ],
    "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
    }
  }